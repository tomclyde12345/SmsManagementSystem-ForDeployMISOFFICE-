

@foreach (var resetpic in Model)
{

    if (resetpic.FilePath == null)
    {
        <img src="~/Images/city-hall.png" id="img_logo" style="width:120px;height:120px;border-radius:100%;" />
    }
    else
    {
        <img src="@resetpic.FilePath" style="width:120px;height:120px;border-radius:100%; "id="img_logo" />
    }

}